<html>
    <head>

    </head>
    <body>
        
        <h1>List of All Student</h1>
        <a href="/student">Add</a>
        <table border="1" align="center" width="800">
            <tr>
                <th>Name</th>
                <th>Fee</th>
                <th>City</th>
                <th>Delete</th>
                <th>Edit</th>
            </tr>

            <% prop.forEach((x)=>{ %>
                <tr>
                    <td><%= x.name %></td>
                    <td><%= x.fee %></td>
                    <td><%= x.city %></td>
                    <td><a href="/delete?id=<%= x._id %>"><button>Del</button></a></td>
                    <td><a href="/edit?id=<%= x._id %>"><button>Edit</button></a></td>
                </tr>
            <% }) %>

        </table>
    </body>
</html>